<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="xmlLayoutResId" format="reference"/>
    <attr name="isRandom" format="boolean"/>
    <attr name="keyDrawable" format="reference"/>
    <declare-styleable name="SystemKeyboard">
        <attr name="xmlLayoutResId"/>
        <attr name="keyDrawable"/>
        <attr name="isRandom"/>
    </declare-styleable>
    <declare-styleable name="SystemKeyBoardEditText">
        <attr name="xmlLayoutResId"/>
        <attr name="keyDrawable"/>
        <attr name="outSideCancel" format="boolean"/>
        <attr name="space" format="boolean"/>
        <attr name="isRandom"/>
    </declare-styleable>
    <attr name="mwhWaves" format="string|enum">
        <enum name="MultiWave" value="-1" />
        <enum name="PairWave" value="-2" />
    </attr>
    <declare-styleable name="MultiWaveHeader">
        <attr name="mwhWaveHeight" format="dimension" />
        <attr name="mwhStartColor" format="color" />
        <attr name="mwhCloseColor" format="color" />
        <attr name="mwhCloseColorSelected" format="color" />
        <attr name="mwhIsRunning" format="boolean" />
        <attr name="mwhColorAlpha" format="fraction|float" />
        <attr name="mwhProgress" format="fraction|float" />
        <attr name="mwhVelocity" format="fraction|float" />
        <attr name="mwhGradientAngle" format="integer" />
        <attr name="mwhWaves" />
    </declare-styleable>
</resources>